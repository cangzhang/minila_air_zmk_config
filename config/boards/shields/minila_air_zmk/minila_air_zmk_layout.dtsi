#include <dt-bindings/zmk/matrix_transform.h>
#include <physical_layouts.dtsi>

/ {
    m66_layout: m66_layout {
        compatible = "zmk,physical-layout";
        display-name = "m66 default layout";
        transform = <&m66>;
        
        keys  //                     w   h    x    y     rot   rx   ry
            = <&key_physical_attrs 100 100    0    0       0    0    0>  // Q
            , <&key_physical_attrs 150 100  100    0       0    0    0>  // Tab (1.5U)
            , <&key_physical_attrs 100 100  250    0       0    0    0>  // A
            , <&key_physical_attrs 100 100  350    0       0    0    0>  // Esc
            , <&key_physical_attrs 100 100  450    0       0    0    0>  // Z
            , <&key_physical_attrs 150 100  600    0       0    0    0>  // \ (1.5U)
            , <&key_physical_attrs 100 100  750    0       0    0    0>  // 1
            , <&key_physical_attrs 100 100    0  100       0    0    0>  // W
            , <&key_physical_attrs 175 100  100  100       0    0    0>  // Caps (1.75U)
            , <&key_physical_attrs 100 100  275  100       0    0    0>  // S
            , <&key_physical_attrs 100 100  475  100       0    0    0>  // X
            , <&key_physical_attrs 100 100  750  100       0    0    0>  // 2
            , <&key_physical_attrs 100 100    0  200       0    0    0>  // E
            , <&key_physical_attrs 100 100  275  200       0    0    0>  // D
            , <&key_physical_attrs 100 100  475  200       0    0    0>  // C
            , <&key_physical_attrs 100 100  750  200       0    0    0>  // 3
            , <&key_physical_attrs 100 100    0  300       0    0    0>  // R
            , <&key_physical_attrs 100 100  100  300       0    0    0>  // T
            , <&key_physical_attrs 100 100  200  300       0    0    0>  // F
            , <&key_physical_attrs 100 100  300  300       0    0    0>  // G
            , <&key_physical_attrs 100 100  400  300       0    0    0>  // V
            , <&key_physical_attrs 100 100  500  300       0    0    0>  // B
            , <&key_physical_attrs 100 100  600  300       0    0    0>  // 5
            , <&key_physical_attrs 100 100  750  300       0    0    0>  // 4
            , <&key_physical_attrs 100 100    0  400       0    0    0>  // U
            , <&key_physical_attrs 100 100  100  400       0    0    0>  // Y
            , <&key_physical_attrs 100 100  200  400       0    0    0>  // J
            , <&key_physical_attrs 100 100  300  400       0    0    0>  // H
            , <&key_physical_attrs 100 100  400  400       0    0    0>  // M
            , <&key_physical_attrs 100 100  500  400       0    0    0>  // N
            , <&key_physical_attrs 100 100  600  400       0    0    0>  // 6
            , <&key_physical_attrs 100 100  750  400       0    0    0>  // 7
            , <&key_physical_attrs 100 100    0  500       0    0    0>  // I
            , <&key_physical_attrs 100 100  100  500       0    0    0>  // ]
            , <&key_physical_attrs 100 100  200  500       0    0    0>  // K
            , <&key_physical_attrs 100 100  400  500       0    0    0>  // ,
            , <&key_physical_attrs 100 100  650  500       0    0    0>  // +/=
            , <&key_physical_attrs 100 100  750  500       0    0    0>  // 8
            , <&key_physical_attrs 100 100    0  600       0    0    0>  // O
            , <&key_physical_attrs 100 100  200  600       0    0    0>  // L
            , <&key_physical_attrs 100 100  400  600       0    0    0>  // .
            , <&key_physical_attrs 100 100  750  600       0    0    0>  // 9
            , <&key_physical_attrs 100 100    0  700       0    0    0>  // P
            , <&key_physical_attrs 100 100  100  700       0    0    0>  // [
            , <&key_physical_attrs 100 100  200  700       0    0    0>  // ;
            , <&key_physical_attrs 100 100  300  700       0    0    0>  // '
            , <&key_physical_attrs 100 100  500  700       0    0    0>  // /
            , <&key_physical_attrs 100 100  650  700       0    0    0>  // -
            , <&key_physical_attrs 100 100  750  700       0    0    0>  // 0
            , <&key_physical_attrs 100 100    0  800       0    0    0>  // R_Fn
            , <&key_physical_attrs 100 100  200  800       0    0    0>  // L_Fn
            , <&key_physical_attrs 100 100  300  800       0    0    0>  // L_Alt
            , <&key_physical_attrs 100 100  500  800       0    0    0>  // R_Alt
            , <&key_physical_attrs 100 100    0  900       0    0    0>  // `
            , <&key_physical_attrs 200 100  100  900       0    0    0>  // Backspace
            , <&key_physical_attrs 225 100  525  900       0    0    0>  // Enter (2.25U)
            , <&key_physical_attrs 100 100  850  900       0    0    0>  // Delete (1U)
            , <&key_physical_attrs 300 100  200 1000       0    0    0>  // Space (3U)
            , <&key_physical_attrs 100 100  500 1000       0    0    0>  // Down
            , <&key_physical_attrs 100 100  500 1100       0    0    0>  // Right
            , <&key_physical_attrs 100 100  100 1200       0    0    0>  // Win
            , <&key_physical_attrs 100 100  500 1200       0    0    0>  // App
            , <&key_physical_attrs 100 100  300 1300       0    0    0>  // UP
            , <&key_physical_attrs 100 100  500 1300       0    0    0>  // Left
            , <&key_physical_attrs 175 100    0 1400       0    0    0>  // L_Ctrl (1.75U)
            , <&key_physical_attrs 225 100  225 1400       0    0    0>  // L_Shift (2.25U)
            , <&key_physical_attrs 100 100  650 1400       0    0    0>  // R_Shift (1U)
            ;
   };

      m66: m66_0 {
            compatible = "zmk,matrix-transform";
            columns = <8>;
            rows = <16>;
            map = <
                RC(1,0) RC(1,1) RC(1,2) RC(1,3) RC(1,4)         RC(1,6) RC(1,7)
                RC(2,0) RC(2,1) RC(2,2)         RC(2,4)                 RC(2,7)
                RC(3,0)         RC(3,2)         RC(3,4)                 RC(3,7)
                RC(4,0) RC(4,1) RC(4,2) RC(4,3) RC(4,4) RC(4,5) RC(4,6) RC(4,7)
                RC(5,0) RC(5,1) RC(5,2) RC(5,3) RC(5,4) RC(5,5) RC(5,6) RC(5,7)
                RC(6,0) RC(6,1) RC(6,2)         RC(6,4)         RC(6,6) RC(6,7)
                RC(7,0)         RC(7,2)         RC(7,4)                 RC(7,7)
                RC(8,0) RC(8,1) RC(8,2) RC(8,3)         RC(8,5) RC(8,6) RC(8,7)
                RC(9,0)         RC(9,2) RC(9,3)         RC(9,5)        
                RC(10,0) RC(10,1)        RC(10,3)         RC(10,6)       
                                RC(11,3)         RC(11,5)               
                                                RC(12,5)               
                        RC(13,1)                RC(13,5)               
                                RC(14,3) RC(14,4) RC(14,5)               
                        RC(15,1) RC(15,2)                              
            >;
        };
};
